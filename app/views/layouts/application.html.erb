<!DOCTYPE html>
<html>
  <head>
    <title>Released</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <%= render 'shared/icons' %>

    <div class="container">
      <%= render 'shared/header' %>

      <div class="main">
        <% flash.each do |key, value| %>
          <div class="flash-message flash-message--<%= key %>">
            <%= value %>
          </div>
        <% end %>

        <%= yield %>
      </div>
    </div>
  </body>
</html>
